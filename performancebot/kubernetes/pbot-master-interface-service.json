{
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
	"name": "pbot-master-interface",
	"labels": {
	    "app": "pbot-master-interface"
	}
    },
    "spec": {
	"type": "NodePort",
	"ports": [
	    {
		"name": "slave-interface",
		"targetPort": 9989,
		"port": 9989
	    },
	    {
		"name": "change-interface",
		"targetPort": 9999,
		"port": 9999
	    }
	],
	"externalIPs": [
	    "192.168.99.100"
	],
	"selector": {
	    "app": "pbot-master"
	}
    }
}
