{
  "apiVersion": "v1",
  "kind": "ReplicationController",
  "metadata": {
    "name": "http-api",
    "labels": {
      "app": "http-api"
    }
  },
  "spec": {
    "replicas": 1,
    "template": {
      "metadata": {
        "labels": {
          "app": "http-api"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "http-api",
            "image": "633007691302.dkr.ecr.us-east-1.amazonaws.com/http-api:latest"
          }
        ]
      }
    }
  }
}
